@model Microsoft.AspNetCore.Identity.IdentityRole

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Are You Sure To Delete This Role?</h1>
<br />
<br />

<div class="row">
    <div class="col-6">
        <div asp-validation-summary="All"></div>
        <form asp-action="Delete" asp-controller="Role" enctype="multipart/form-data">
            <partial name="PartialViews/DetailsDeleteRolePartialView" />
            <br />
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-light">Back To List</a>

        </form>
    </div>
</div>

@section Scripts {

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>


}




